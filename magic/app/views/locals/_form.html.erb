<%= simple_form_for(@local) do |f| %>
  <%  f.error_notification %>

  <div class="field">
    <%= f.input :name %>
  </div>
  <div class="field">

    <%= f.input :address %>
  </div>
  
  <div class="field">

    <%= f.input :telefono %>
  </div>
  <div class="field">

    <%= f.association :district %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
